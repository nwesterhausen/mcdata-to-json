{
  "name": "mcdata-to-json",
  "version": "1.0.0",
  "description": "Node.js app to turn Minecraft .dat and .json files into unified json.",
  "keywords": [
    "minecraft",
    "json",
    "nbt",
    "mca",
    "cli"
  ],
  "main": "index.js",
  "bin": {
    "mcdata-to-json": ".dist/index.js"
  },
  "scripts": {
    "check-lint": "eslint index.js lib",
    "check-pretty": "prettier --check index.js lib README.md docs",
    "lint": "eslint --fix index.js lib",
    "pretty": "prettier --write index.js lib README.md docs",
    "test": "echo \"Error: no test specified\" && exit 0",
    "tidy-up": "pnpm lint && pnpm pretty",
    "watch": "tsc -w"
  },
  "author": "Nicholas Westerhausen",
  "license": "MIT",
  "repository": "github:nwesterhausen/mcdata-to-json",
  "homepage": "https://github.com/nwesterhausen/mcdata-to-json",
  "dependencies": {
    "7zip-bin": "^5.0.3",
    "axios": "^0.20.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "int64-napi": "^1.0.1",
    "lodash": "^4.17.20",
    "mca-js": "^0.1.1",
    "nbt": "^0.8.1",
    "node-7z": "^2.1.2",
    "winston": "^3.3.3",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/node": "^14.14.6",
    "eslint": "^7.12.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
